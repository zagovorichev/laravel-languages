{
  "name": "zagovorichev/laravel-languages",
  "description": "Using different languages in the Laravel",
  "minimum-stability": "dev",
  "license": ["MIT"],
  "authors": [
    {
      "name": "Alexander Zagovorichev",
      "email": "zagovorichev@gmail.com"
    }
  ],
